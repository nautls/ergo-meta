{
  "type": "object",
  "properties": {
    "$schema": {
      "type": "string",
      "const": "./token-metadata.schema.json"
    },
    "name": {
      "type": "string",
      "minLength": 1,
      "maxLength": 50
    },
    "description": {
      "type": "string",
      "maxLength": 500
    },
    "url": {
      "type": "string",
      "maxLength": 250
    },
    "tokenId": {
      "type": "string",
      "minLength": 64,
      "maxLength": 64
    },
    "ticker": {
      "type": "string",
      "minLength": 2,
      "maxLength": 9
    },
    "decimals": {
      "type": "integer",
      "minimum": 0,
      "maximum": 19
    },
    "logo": {
      "type": "string",
      "maxLength": 87400
    }
  },
  "required": [
    "name",
    "tokenId"
  ],
  "additionalProperties": false,
  "$schema": "http://json-schema.org/draft-07/schema#"
}